<div class="logged-in-img">
  <a href="/home">
    <img class="non-home-progile-img" src={{user.imgPath}}>
  </a>
</div>

<h5 class="headline">Create a new note</h5>
<div class="row container-form">
  <form action="/notes" method="POST" class="col s12">
    <div class="row">
      <div class="inputField col s12">
        <input name="title" type="text" class="validate">
        <label for="title">Title</label>
      </div>
    </div>
    <div class="row">
      <div class="inputField col s12">
        <textarea name="text" id="textarea1" class="materialize-textarea"></textarea>
        <label for="text">Text</label>
      </div>
    </div>
    <div id="inputField"></div>
    <!-- Chips  -->
    <div name="tags" id="chips" class="chips"></div>
    <button type="submit" class="btn btn-info submit-update">Create</button>
  </form>
</div>


{{#each tags}}
<p class="all-tags">{{name}}</p>
{{/each}}